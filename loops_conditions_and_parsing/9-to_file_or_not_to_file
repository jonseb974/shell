#!/usr/bin/env bash
# give info about file school
File=school file
if [ -f $"File" ]
	echo $"school file exists"
then
	if [ ! -f $"File" ]
		echo $"school file does not exists"
	then
		if [ -s $"File"]
			echo "school file is not empty"
		then
			if [ !-s $"File" ]
				echo "school file is empty"
			else echo "school file is a regular file"
			fi 
		fi 
	fi 
fi



